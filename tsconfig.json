{
  "compilerOptions": {
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "target": "es5",
    "outDir": "dist",
    "sourceMap": true,
    "lib": [
      "es2015",
      "es2016.array.include"
    ]
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "include": [
    "./src/**/*.ts",
    "./tests/**/*.ts",
    "./custom.d.ts"
  ]
}